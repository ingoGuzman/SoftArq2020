					var	revapi2,
						tpj;
					function revinit_sliderHome() {
					jQuery(function() {
						tpj = jQuery;
						revapi2 = tpj("#sliderHome");
						if(revapi2==undefined || revapi2.revolution == undefined){
							revslider_showDoubleJqueryError("sliderHome");
						}else{
							revapi2.revolution({
								sliderLayout:"fullwidth",
								visibilityLevels:"1240,1240,1240,480",
								gridwidth:"1600,1600,1600,480",
